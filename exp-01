# Hard-coded values
cost_per_attendee = 50  # Cost per attendee (m)
num_attendees = 100     # Number of attendees (x)
fixed_cost = 500        # Fixed costs (b)

# Calculate total cost
total_cost = cost_per_attendee * num_attendees + fixed_cost

# Display the total cost
print(f"Cost per Attendee: ${cost_per_attendee}")
print(f"Number of Attendees: {num_attendees}")
print(f"Fixed Cost: ${fixed_cost}")
print(f"Total Cost:Â ${total_cost}")
